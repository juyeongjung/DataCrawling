<script type="text/javascript">
//<![CDATA[
var resizeIframe = function (){
    $("#contentsFrame").load(function(){
        $(this).height($(this).contents().find('html')[0].scrollHeight);
    });
}
  jQuery(function(){
   resizeIframe();
   jQuery('a[target*="_blank"],area[target*="_blank"],a[target*="new"],a[onclick*="window.open"]').each(function(index, el) {
     $(this).attr('title','새창열림')
   });
   $('button').each(function(index, el) {
     if ($(this).attr('title')==undefined) {
      $(this).attr('title',$(this).find('img').attr('alt'));
     };
   });
   $('label').each(function(index, el) {
     if ($(this).find('img').attr('alt')!=undefined) {
      $('#'+$(this).attr('for')).attr('title',$(this).find('img').attr('alt'))
     };
   });
  });
//]]>
</script>
