<script type="text/javascript">
	//common rollover image kth
	function imgOn(item) {
	    var image = item.find("img");
	    var imgsrc = jQuery(image).attr("src");
	    if (jQuery(image).length) {
	        var on = imgsrc.replace(/_off/, "_on");
	        jQuery(image).attr("src", on);
	    };
	};

	function imgOff(item) {
	    var image = item.find("img");
	    for (var i = 0; i < image.length; i++) {
	        var imgsrc = jQuery(image[i]).attr("src");
	        var off = imgsrc.replace(/_on/, "_off");
	        jQuery(image[i]).attr("src", off);
	    };
	};

jQuery(function() {
	$('#page_header .gnb > li > a').click(function() {
		var thisLi = this.parentNode;
		$('#page_header .gnb > li > div').each(function() {
			if(this.parentNode == thisLi) {
				$(this).slideToggle();
			} else {
				$(this).hide();
				var img = $('> a > img', this.parentNode).get(0);
				img.src = img.src.replace('_on.gif', '_off.gif');
			}
		});
		return $('> div', this.parentNode).length <= 0;
	}).bind('mouseenter keyup', function(event) {
		//imgOff(gnb_link_depth1);
		imgOn(jQuery(this));

	}).bind('mouseleave blur', function(event) {
		/* Act on the event */
		imgOff(jQuery(this));
	});

	$('#page_header .gnb > li > div > p').click(function() {
		$(this.parentNode).slideUp();
		$(this).parents('li').find('a:first').focus();
		return false;
	}).mouseenter(function() {
		$(this).addClass('on');
	}).mouseleave(function() {
		$(this).removeClass('on');
	});
	$('#page_header ul.depth-02 > li').mouseenter(function() {
		$(this).addClass('on');
	}).mouseleave(function() {
		$(this).removeClass('on');
	});

	if (document.documentMode) {	// IE브라우저의 경우 360VR 안내페이지 띄우기
		$('.vr360').attr('href', '/site/about/guide/KMU.html');
	} else {	// 기타 브라우저의 경우 바로 링크 걸기
		$('.vr360').attr('href', '/site/about/guide/KMU_VR.html');
	}
});
</script>
